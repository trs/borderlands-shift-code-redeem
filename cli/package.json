{
  "name": "@shift-code/cli",
  "version": "0.1.2",
  "description": "Redeem Borderlands SHiFT codes automatically",
  "main": "./dist/bin",
  "bin": {
    "shift-code": "./dist/bin.js"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "pkg": "pkg ./package.json --out-path pkg"
  },
  "engines": {
    "node": ">= 10"
  },
  "keywords": [
    "borderlands",
    "shift",
    "code",
    "redeem"
  ],
  "pkg": {
    "targets": [
      "node12-macos",
      "node12-linux",
      "node12-win"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/mkdirp": "^1.0.0",
    "@types/node": "^12.7.4",
    "@types/prompts": "^2.0.8",
    "@types/signale": "^1.4.1",
    "pkg": "^4.4.8",
    "ts-node": "^9.0.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@shift-code/api": "^0.1.0",
    "@shift-code/get": "^0.1.1",
    "commander": "^6.1.0",
    "mkdirp": "^1.0.4",
    "prompts": "^2.3.2",
    "signale": "^1.4.0"
  }
}
